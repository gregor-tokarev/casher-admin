{
  "parser": "vue-eslint-parser",
  "root": true,
  "env": {
    "browser": true
  },
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  },
  "extends": ["@nuxtjs/eslint-config-typescript", "plugin:prettier/recommended"],
  "rules": {
    "vue/multi-word-component-names": "off",
    "no-useless-return": "off"
  }
}
